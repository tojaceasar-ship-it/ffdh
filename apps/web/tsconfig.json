{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "../..",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ],
      "@/types/*": [
        "./src/types/*"
      ],
      "@/utils/*": [
        "./src/utils/*"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ],
      "@/services/*": [
        "./src/services/*"
      ],
      "@/store/*": [
        "./src/store/*"
      ],
      "@/contexts/*": [
        "./src/contexts/*"
      ]
    }
  },
  "include": [
    "../../app/**/*.ts",
    "../../app/**/*.tsx",
    "../../src/**/*.ts",
    "../../src/**/*.tsx",
    ".next/types/**/*.ts"
  ]
}
